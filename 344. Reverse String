class Solution {
public:
    string reverseString(string s) {
        int left = 0;
        int right = s.size()-1;
        while(left<right){
            swap(s[left++],s[right--]);
        }
        return s;
    }
};

class Solution {
public:
    string reverseString(string s) {
        return string(s.rbegin(),s.rend());
    }
};
