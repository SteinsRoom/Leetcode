class Solution {
public:
    vector<int> nextGreaterElement(vector<int>& findNums, vector<int>& nums) {
        vector<int> result;
        for(auto i : findNums){
            auto it = find(nums.begin(),nums.end(),i);
            it++;
            while(it!=nums.end()){
                if(*it<=i){
                    it++;
                }else{
                    result.push_back(*it);
                    break;
                }
            }
            if(it == nums.end()){
                result.push_back(-1);
            }
        }
        return result;
    }
};
