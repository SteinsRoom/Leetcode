class Solution {
public:
    ListNode* oddEvenList(ListNode* head) {
        if(head == NULL) return head;
        auto odd = head;
        auto evenhead = head->next;
        auto even = evenhead;
        while(even && even ->next){
            odd->next = odd->next->next;
            even->next= even->next->next;
            odd = odd->next;
            even = even->next;
        }
        odd->next =evenhead;
        return head;
        
    }
};
